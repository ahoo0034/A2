{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A population pyramid for the US in 2000.",
    "data": { "url": "https://raw.githubusercontent.com/ahoo0034/A2/refs/heads/main/Data/Australia's_population_born_Australia_overseas.csv"},
   
    "spacing": 0,
    "hconcat": [{
      
      "title": "Aust-born females(%)",
      "mark": "bar",
      "encoding": {
        "y": {
          "field": "Age group(years)", "axis": null, "sort": "descending"
        },
        "x": {
          "aggregate": "sum", "field": "Aust-born females(%)",
          "title": "Aust-born females(%)",
          "axis": {"format": "s"},
          "sort": "descending"
        },
        "color": {
          "field": "gender",
          "scale": {"range": ["#675193", "#ca8861"]},
          "legend": null
        },
        "tooltip": [
          { "field": "Age group(years)", "type": "nominal", "title": "Age group(years)" },
          { "field": "Aust-born females(%)", "type": "quantitative", "title": "Aust-born females(%)" }
        ]
      }
    }, {
      "width": 20,
      "view": {"stroke": null},
      "mark": {
        "type": "text",
        "align": "center"
      },
      "encoding": {
        "y": {"field": "Age group(years)", "type": "ordinal", "axis": null, "sort": "descending"},
        "text": {"field": "Age group(years)", "type": "quantitative"}
        
      }
    }, {
      
      "title": "OS-born females(%)",
      "mark": "bar",
      "encoding": {
        "y": {
          "field": "Age group(years)", "title": null,
          "axis": null, "sort": "descending"
        },
        "x": {
          "aggregate": "sum", "field": "OS-born females(%)",
          "title": "OS-born females(%)",
          "axis": {"format": "s"}
        },
        "color": {
          "field": "gender",
          "legend": null
        },
        "tooltip": [
          { "field": "Age group(years)", "type": "nominal", "title": "Age group(years)" },
          { "field": "OS-born females(%)", "type": "quantitative", "title": "OS-born females(%)" }
          ]
      }
    }],
    "config": {
      "view": {"stroke": null},
      "axis": {"grid": false}
    }
  }
  